    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@types/autosize": "^3.0.7",
    "@types/micromodal": "^0.3.1",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3"
